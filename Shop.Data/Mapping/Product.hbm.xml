<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Shop.Data" namespace="Shop.Data.Models">
	<class name="Shop.Data.Models.Product" table="Products" >
		<id name="Id" column="Id" >
			<generator class="native" />
		</id>
		<property name="Name" />
		<property name="Price" />
		<property name="Description" />
		<property name="Quantity" />
		<property name="ImageUrl" />
		<property name="CategoryId" />
	</class>
</hibernate-mapping>